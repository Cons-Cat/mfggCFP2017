    <event eventtype="3" enumb="0">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>event_inherited();

if dead = 0 || dead = 2{
    if dead = 0{
        if dir = 1{ //Moving right
            c_hspeed = spd;
        }
        if dir = -1{ //Moving right
            c_hspeed = -spd;
        }
        
        if canTurn = 1{
            if !collision_rectangle(bbox_left,bbox_bottom,bbox_left+8,bbox_bottom+15,obj_solid_parent,false,false){
                dir = 1;
            }
            if !collision_rectangle(bbox_right,bbox_bottom,bbox_right-8,bbox_bottom+15,obj_solid_parent,false,false){
                dir = -1;
            }
        }
    }
    if dead = 2{
        if holding = 0{
            if kicked = 1{
                if dir = 1{ //Moving right
                    c_hspeed = kickSpd;
                }
                if dir = -1{ //Moving right
                    c_hspeed = -kickSpd;
                }
            }
        }
    }
    image_xscale = dir;
    image_speed = c_hspeed/10;
    
    with collision_rectangle(bbox_right,bbox_top,bbox_left,bbox_bottom,obj_enemy_parent,false,true){
        if dead = 2 &amp;&amp; kicked = 1{
            other.dead = 3;
            other.c_vspeed = -4;
        }
    }
    
    with collision_rectangle(bbox_left,bbox_top,bbox_right,bbox_top+1,obj_player,false,true){
        if c_vspeed &gt; 0{
            if (y + sprite_height/2 &lt; other.y+8) || (other.dead = 0){
                if (other.canJumped = 1 &amp;&amp; other.kicked = 1) || (other.canJumped = 0) || (other.canJumped = 1 &amp;&amp; other.dead = 0){
                    if keyboard_check(global.p1_a){
                        c_vspeed = -(phy_jumpheight+0.5);
                    } else {
                        c_vspeed = -(phy_jumpheight-1);
                    }
                   
                    jumping = 1;
                    other.trg = self.id;
                }
                other.c_hspeed = 0;
                
                if other.canJumped = 0{
                    other.dead = 1; //Death
                    holding = 1;
                }
                if other.canJumped = 1{
                    other.dead = 2; //Shell
                    other.kicked = 0;
                    other.kickDeadly = 0;
                    other.canHold = 1;
                }
            }
        }
    }
}
        
if dead = 1{
    //Death upon jumping
    c_hspeed = 0;
    image_speed = 0;
    sprite_index = deathSpr1;
    
    if deathGone = 0{
        deathGone = 1;
        alarm[0] = deathTime;
    }
}
if dead = 2{
    //Hiding into a shell
    sprite_index = deathSpr1;
    
    if kicked = 0{
        if c_vspeed &gt;= 0{
            if collision_rectangle(bbox_left,bbox_top+8,bbox_right,bbox_bottom,trg,false,false){
                if !keyboard_check(global.p1_b){
                    if trg != 0{
                        if trg.x &gt;= self.x{
                            dir = -1;
                        }
                        if trg.x &lt; self.x{
                            dir = 1;
                        }
                        canHold = 0;
                    }
                    
                    kicked = 1;
                    alarm[1] = 10;
                } else {
                    if trg.vspeed &lt;= 0{
                        holding = 1;
                    }
                }
            }
        }
    }
}

if dead = 3{
    //Death by item
    c_hspeed = 0;
    image_yscale = -1;
    image_speed = 0;
}

if y &gt; room_height{
    instance_destroy();
}

</string>
          </argument>
        </arguments>
      </action>
    </event>
